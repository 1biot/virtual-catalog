{
  "name": "nette/web-project",
  "description": "Nette: Standard Web Project",
  "keywords": ["nette"],
  "type": "project",
  "license": ["MIT", "BSD-3-Clause", "GPL-2.0-only", "GPL-3.0-only"],
  "require": {
    "php": ">= 8.1",
    "ext-curl": "*",
    "nette/application": "^3.2.3",
    "nette/bootstrap": "^3.2",
    "nette/caching": "^3.2",
    "nette/database": "^3.2",
    "nette/di": "^3.2",
    "nette/forms": "^3.2",
    "nette/http": "^3.3",
    "nette/mail": "^4.0",
    "nette/robot-loader": "^4.0",
    "nette/security": "^3.2",
    "nette/utils": "^4.0",
    "latte/latte": "^3.0",
    "tracy/tracy": "^2.10",
    "1biot/fiquela": "^2.0",
    "halaxa/json-machine": "^1.2"
  },
  "require-dev": {
    "nette/tester": "^2.5",
    "symfony/thanks": "^1",
    "vlucas/phpdotenv": "^5.6"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "symfony/thanks": true
    }
  },
  "scripts": {
    "catalogue:download": [
      "./bin/download.sh"
    ],
    "catalogue:clean:cache": [
      "find ./temp/cache -mindepth 1 ! -name 'products.xml' -delete"
    ],
    "catalogue:clean:temp": [
      "rm -rf ./temp/*"
    ],
    "dev:start": [
      "php -S localhost:8000 -t public"
    ]
  }
}
